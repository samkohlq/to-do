{
  "name": "todo-backend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel -d ./build ./src -s --copy-files",
    "clean": "rm -rf build",
    "start": "NODE_ENV=PRODUCTION npm run build && node ./build/bin/www",
    "start:dev": "nodemon --exec babel-node ./src/bin/www"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "regenerator-runtime": "^0.13.3",
    "sequelize": "^5.21.4",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "@babel/node": "^7.8.4",
    "nodemon": "^2.0.2"
  }
}
